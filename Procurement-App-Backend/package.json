    "mongoose": "^5.11.6",
    "socket.io": "^4.4.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.18",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
